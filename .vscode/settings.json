{
  "files.exclude": {
    "npm-debug.log": true,
    ".testresults": true,
    "**/node_modules": true,
    "**/*.tgz*": true,
    "**/*.tar.*": true
  },
  "cSpell.enabled": true,
  "cSpell.ignorePaths": [
    ".testresults",
    "**/.git/**",
    ".vscode",
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "azure-pipelines",
    "html",
    "javascript",
    "json",
    "markdown",
    "plaintext",
    "properties",
    "shellscript",
    "text",
    "yaml"
  ],
  "cSpell.dictionaries": [
    "companies",
    "en_us",
    "filetypes",
    "misc",
    "node",
    "npm",
    "softwareTerms"
  ],
  "files.associations": {
    "**/.azure-pipelines/**": "azure-pipelines"
  },
}
